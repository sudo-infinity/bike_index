%section.full-screen-table
  %h2
    Bike Index graphs

  %table.table.table-striped.table-bordered
    %tbody
      - ["users","bikes","organizations"].each do |graph_type|
        %tr
          %td
            = graph_type.humanize
          %td
            = link_to "Since start", admin_graph_url("#{graph_type}")
      %tr
        %td
          This week in review
        %td
          = link_to "Past week", review_admin_graphs_url